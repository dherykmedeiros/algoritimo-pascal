program q1;
var
  n1, n2, n3, n4, soma: integer;
begin
  writeln('Digite o primeiro numero');
  readln(n1);
  writeln('Digite o segundo numero');
  readln(n2);
  writeln('Digite o terceiro numero');
  readln(n3);
  writeln('Digite o quarto numero');
  readln(n4);
  soma:= n1 + n2 + n3 + n4;
  writeln('O resultado e: ', soma);

end.

